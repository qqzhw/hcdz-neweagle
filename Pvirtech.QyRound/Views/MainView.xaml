                        </telerik:RadGridView.ContextMenu>
                        <telerik:EventToCommandBehavior.EventBindings>
                            <telerik:EventBinding Command="{Binding DataContext.SDKVm.ListRightMenue,RelativeSource={RelativeSource Mode=FindAncestor,AncestorType=UserControl}}" EventName="MouseRightButtonUp" 
                        PassEventArgsToCommand="True" />
                            <telerik:EventBinding Command="{Binding DataContext.DoubleClickCmd ,RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=UserControl}}"    EventName="MouseDoubleClick" 
                        PassEventArgsToCommand="True" />
                        </telerik:EventToCommandBehavior.EventBindings>
                        <telerik:RadGridView.Columns>
                            <telerik:GridViewDataColumn Header="任务名称"  Width="Auto"     HeaderTextAlignment="Justify"  TextBlock.FontWeight="SemiBold"  DataMemberBinding="{Binding  TaskName,Mode =OneWay}"/>

                            <telerik:GridViewDataColumn Header="记录名称"  Width="*"      HeaderTextAlignment="Justify"    DataMemberBinding="{Binding  RecordName,Mode =OneWay}"/>
 
                           
                            <telerik:GridViewDataColumn Header="大小" DataMemberBinding="{Binding TotalSize,Mode=OneWay}" TextWrapping="Wrap" Width="80" />

                            <telerik:GridViewDataColumn Header="创建时间" Width="160">
                                <telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate >
                                        <TextBlock Text="{Binding StartTimeText,Mode=OneWay}" />
                                    </DataTemplate>
                                </telerik:GridViewDataColumn.CellTemplate>
                            </telerik:GridViewDataColumn>
                            <!--<telerik:GridViewDataColumn Header="结束时间" Width="160"  HeaderTextAlignment="Center" TextAlignment="Center" DataMemberBinding="{Binding EndTimeText,Mode=OneWay}">

                            </telerik:GridViewDataColumn>-->


                        </telerik:RadGridView.Columns>

                        <telerik:RadGridView.GroupDescriptors >
                            <telerik:ColumnGroupDescriptor  telerik:StyleManager.Theme="Windows10" Column="{Binding Columns[\TaskName\], ElementName=dgInfo}"
											   SortDirection="Descending" />
                        </telerik:RadGridView.GroupDescriptors>

                    </telerik:RadGridView>
                </Grid>
            </Border>
        </Grid>
     
    </Grid>
    
</UserControl>
